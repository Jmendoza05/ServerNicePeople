{
    "version": 2,
    "builds":[
        {
            "src":".index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/api/contenido(.*)",
            "dest": "/"
        }
    ],
    ,
     "rewrites": [{
    "source": "/api/contenidos/(.*)", "destination": "/api/app.js"
  }]
}
